{
  "name": "@gjv/lib-core",
  "version": "0.4.2",
  "private": true,
  "description": "Library of shared code",
  "author": "Greg Valentine <gregjoeval@gmail.com>",
  "homepage": "https://github.com/gregjoeval/package-library#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "setup:install": "npm install",
    "setup:clean-install": "npm ci",
    "setup:clean": "rimraf ./node_modules",
    "build": "tsc",
    "build:clean": "rimraf ./dist",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx  --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests yet!\" && exit 0",
    "check": "npm run lint && npm run typecheck && npm run test",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregjoeval/package-library.git"
  },
  "bugs": {
    "url": "https://github.com/gregjoeval/package-library/issues"
  },
  "devDependencies": {
    "@gjv/eslint-config": "^0.4.1",
    "@gjv/eslint-config-typescript": "^0.4.2",
    "tsdx": "^0.13.2"
  }
}
